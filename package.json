  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-release": "npm run web-build && cargo build --release",
    "web-dev": "tsc && vite build --watch",
    "server-dev": "nodemon --watch ./ -e rs,toml --exec 'cargo run' ",
