    "@types/alpinejs": "^3.13.5",
    "@types/node": "^20.10.5",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@apache-arrow/ts": "^14.0.2",
    "alpinejs": "^3.13.3",
    "apache-arrow": "^14.0.2",
    "maplibre-gl": "^3.6.2"
  }
}
